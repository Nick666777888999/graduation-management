{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index-new-api.html", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/new",
      "dest": "/index-new-api.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
